#!/bin/bash
echo $ProviderSwitch3.MAC("Link17") - Controller &gt;&gt; topology.txt
echo $ProviderSwitch3.MAC("Link20") - ProviderSwitch2 &gt;&gt; topology.txt
echo $ProviderSwitch3.MAC("Link21") - MediaServer &gt;&gt; topology.txt
echo $ProviderSwitch3.MAC("Link2") - ProviderSwitch1 &gt;&gt; topology.txt
# Get Interface Mapping
cd /
wget http://www4.ncsu.edu/~rnudechu/files/GENI/EPB/topologyIntf.sh
chmod +x topologyIntf.sh
./topologyIntf.sh topology.txt &gt;&gt; /mapping.txt
# SDN Controller
apt-get update
apt-get install unzip gcc python-setuptools python-greenlet -y
# Move the controller code
wget http://www4.ncsu.edu/~rnudechu/files/GENI/EPB/Controller/Ryu.zip
unzip /Ryu.zip
cd /Ryu-master
python ./setup.py install 
cd /
wget http://www4.ncsu.edu/~rnudechu/files/GENI/ChoiceNet/Demo/rnu_epb_controller.py
wget http://www4.ncsu.edu/~rnudechu/files/GENI/ChoiceNet/Demo/firewallParser.py
wget http://www4.ncsu.edu/~rnudechu/files/GENI/ChoiceNet/Demo/config-p3v2.ini
mv config-p3v2.ini config.ini
# Auto-startingn the controller is not working ...
#ryu-manager rnu_epb_controller.py &amp;
#Dynamically create a config file for LINC
wget http://www4.ncsu.edu/~rnudechu/files/GENI/ChoiceNet/Demo/createLINCConfigFilev2.sh
mv createLINCConfigFilev2.sh createLINCConfigFile.sh
chmod +x createLINCConfigFile.sh
./createLINCConfigFile.sh /mapping.txt
mv sys.config /LINC-Switch-master/rel/files/
#wget http://www4.ncsu.edu/~rnudechu/files/GENI/EPB/LINC/main.config 
#mv main.config rel/files/sys.config
# Install LINC
cd /LINC-Switch-master
make offline
