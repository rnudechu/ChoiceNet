# Install Wireshark and other utility files
sudo apt-get install wireshark wireshark-dev unzip vlan -y
# Download Sample Traffic Shaper
wget http://www4.ncsu.edu/~rnudechu/files/GENI/ChoiceNet/Demo/firewallParser.py
wget http://www4.ncsu.edu/~rnudechu/files/GENI/ChoiceNet/Demo/application-payload-servicev2.py
# Bring up the interfaces
ifconfig eth1 up promisc
ifconfig eth2 up promisc
ifconfig eth3 up promisc
#!/bin/bash
echo $EPB.MAC("Link9") - To EPB &gt;&gt; topology.txt
echo $EPB.MAC("Link10") - Out EPB  &gt;&gt; topology.txt
echo $EPB.MAC("Link11") - EPB Management  &gt;&gt; topology.txt
# Get Interface Mapping
cd /
wget http://www4.ncsu.edu/~rnudechu/files/GENI/EPB/topologyIntf.sh
chmod +x topologyIntf.sh
./topologyIntf.sh topology.txt &gt;&gt; mapping.txt
# Setup the Interface Mapping
./setupEPBIntf.sh  mapping.txt</request-schema:postBootScript>
