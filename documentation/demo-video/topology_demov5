# Ubuntu 12.04
# Install CouchDB
apt-get update
apt-get install couchdb -y
apt-get install curl -y
apt-get install xterm openjdk-6-jre-headless openjdk-6-jre -y
# make port accessible to everyone, copy your couchdb config file
wget http://www4.ncsu.edu/~rnudechu/files/GENI/CouchDB/default.ini
mv default.ini /etc/couchdb
# Reload CouchDB
# Download my script for jump starting that feature
wget http://www4.ncsu.edu/~rnudechu/files/GENI/CouchDB/updateCouchDB.sh
# Download the ChoiceNet Provider program along with its config file
wget http://www4.ncsu.edu/~rnudechu/files/GENI/ChoiceNet/Demo/marketplace.properties
# Used by the Admin tool
wget http://www4.ncsu.edu/~rnudechu/files/GENI/ChoiceNet/Demo/marketplace_view.json
wget http://www4.ncsu.edu/~rnudechu/files/GENI/ChoiceNet/GEC21Demo/adminGUI.jar
wget http://www4.ncsu.edu/~rnudechu/files/GENI/ChoiceNet/GEC21Demo/admin.properties
wget http://www4.ncsu.edu/~rnudechu/files/GENI/ChoiceNet/Demo/ChoiceNetSpeaker.jar
mkdir ChoiceNet
mv /updateCouchDB.sh ChoiceNet
mv /adminGUI.jar ChoiceNet
mv /marketplace_view.json ChoiceNet
mv /marketplace.properties ChoiceNet
mv /admin.properties ChoiceNet
mv /ChoiceNetSpeaker.jar ChoiceNet
bash /ChoiceNet/updateCouchDB.sh
#!/bin/bash
arp -s 10.2.1.100 $Client.MAC("Link32")
arp -s 10.2.1.4 $Planner.MAC("Link0")
arp -s 10.2.1.1 $Provider.MAC("Link34")
arp -s 10.2.1.2 $Provider2.MAC("Link24")
arp -s 10.2.1.3 $Provider3.MAC("Link14")
# Install LAMP
wget http://www4.ncsu.edu/~rnudechu/files/GENI/ChoiceNet/GEC23Demo/paymentPortal.sh
chmod +x paymentPortal.sh
./paymentPortal.sh
cd /
wget http://www4.ncsu.edu/~rnudechu/files/GENI/ChoiceNet/Demo/marketplaceProcessingAgent.sh
chmod +x marketplaceProcessingAgent.sh
./marketplaceProcessingAgent.sh</request-schema:postBootScript>
